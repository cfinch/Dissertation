	Month = {May 1, 1998},
	Number = {18},
	Pages = {10988-10993},
	Shorttitle = {Force Required to Break alpha 5beta 1 Integrin-Fibronectin Bonds in Intact Adherent Cells Is Sensitive to Integrin Activation State},
	Title = {Force Required to Break alpha 5beta 1 Integrin-Fibronectin Bonds in Intact Adherent Cells Is Sensitive to Integrin Activation State},
	Url = {http://www.jbc.org/cgi/content/abstract/273/18/10988},
	Volume = {273},
