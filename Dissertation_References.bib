	Keywords = {Neuronal culture Hippocampus Rat embryo X-ray photoelectron spectroscopy Surface Serum-free medium Protein adsorption},
	Note = {doi: DOI: 10.1016/0165-0270(95)00063-1},
	Number = {1-2},
	Pages = {111-119},
	Shorttitle = {Investigation of the factors necessary for growth of hippocampal neurons in a defined system},
	Title = {Investigation of the factors necessary for growth of hippocampal neurons in a defined system},
	Url = {http://www.sciencedirect.com/science/article/B6T04-3W0P6N6-1D/2/b04f5e9d5a4b552a2dac3271534c7c20},
